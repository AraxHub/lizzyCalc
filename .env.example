# Пример файла .env. Скопируй в .env и подставь свои значения.
# .env не коммитить в git (добавь в .gitignore).
# Префикс APP задаётся в коде (Process("APP", &cfg)).

APP_LOG_LEVEL=info

# Сервер (вложенная структура SERVER)
APP_SERVER_HOST=0.0.0.0
APP_SERVER_PORT=8080

# БД (вложенная структура DB)
APP_DB_HOST=localhost
APP_DB_PORT=5433
APP_DB_USER=postgres
APP_DB_PASSWORD=postgres
APP_DB_NAME=lizzycalc
APP_DB_SSLMODE=disable
